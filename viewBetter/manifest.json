{
  "manifest_version": 3,
  "name": "ViewBetter",
  "description": "Extension for better accessibility",
  "version": "1.0",
  "action": {
    "default_popup": "popup.html"
  },
  "permissions": ["tabs", "scripting", "identity", "identity.email"],
  "host_permissions": ["http://*/*", "https://*/*"],
  "icons": {
    "128": "extension-icon.png"
  }
}
